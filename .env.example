FLASK_APP=wsgi.py
FLASK_ENV=development
SECRET_KEY=your-secret-key-here
DATABASE_URL=sqlite:///app.db

# Photo Storage Configuration
S3_PHOTOS_BUCKET=collecting-photos-dev
S3_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key

# Photo upload constraints (optional - defaults provided)
MAX_PHOTO_SIZE_MB=5
MAX_PHOTOS_PER_GAME=20
PHOTO_UPLOAD_EXPIRY_MINUTES=15
